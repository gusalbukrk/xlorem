{
  "name": "api",
  "author": "gusalbukrk",
  "version": "0.0.0",
  "description": "",
  "license": "MIT",
  "keywords": [],
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/bundle.cjs",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/bundle.js",
      "require": "./dist/bundle.cjs"
    },
    "./src": "./src/index.ts"
  },
  "files": [
    "dist",
    "src",
    "!src/**/__tests__/",
    "!src/**/__spec__/",
    "!src/**/*.test.[jt]s",
    "!src/**/*.test.[jt]sx",
    "!src/**/*.spec.[jt]s",
    "!src/**/*.spec.[jt]sx"
  ],
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "lint": "eslint src/ --fix && cspell \"**\" -c ../../cspell.config.cjs --no-progress",
    "postbuild": "eslint dist/index.d.ts --fix --no-ignore --no-error-on-unmatched-pattern",
    "postpublish": "../../node_modules/rimraf/bin.js dist/",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "express": "^4.18.1",
    "xlorem": "file:../xlorem",
    "xlorem-common": "file:../common"
  }
}
