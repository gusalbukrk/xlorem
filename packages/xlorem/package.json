{
  "name": "xlorem",
  "author": "gusalbukrk",
  "version": "0.0.0",
  "description": "Filler text generator.",
  "license": "MIT",
  "keywords": [
    "xlorem",
    "text",
    "lorem",
    "ipsum",
    "filler",
    "dummy",
    "placeholder",
    "mockup"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/bundle.cjs.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": "./dist/bundle.esm.js",
    "require": "./dist/bundle.cjs.js"
  },
  "files": [
    "dist",
    "src",
    "!src/**/__tests__",
    "!src/**/*.{test,spec}.{js,jsx,ts,tsx}"
  ],
  "scripts": {
    "build": "rollup -c ../../rollup.config.js",
    "lint": "eslint src/ --fix",
    "postbuild": "eslint dist/index.d.ts --fix --no-ignore && replace-in-file --configFile=../../.replace-in-file.cjs",
    "postpublish": "../../node_modules/rimraf/bin.js dist/",
    "prepublishOnly": "npm run build",
    "test": "jest  --passWithNoTests"
  },
  "dependencies": {
    "@xlorem/common": "file:../common",
    "@xlorem/input-validator": "file:../input-validator",
    "cross-fetch": "^3.1.4",
    "generate-random-text": "file:../generate-random-text",
    "generate-words-freqmap": "file:../generate-words-freqmap",
    "get-wikipedia-article": "file:../get-wikipedia-article",
    "stopwords-utils": "file:../stopwords-utils",
    "tokenize-words": "file:../tokenize-words"
  }
}
