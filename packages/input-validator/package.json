{
  "name": "@xlorem/input-validator",
  "author": "gusalbukrk",
  "version": "0.0.0",
  "description": "Validates input passed to xlorem.",
  "license": "MIT",
  "keywords": [
    "xlorem"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/bundle.cjs.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": "./dist/bundle.esm.js",
    "require": "./dist/bundle.cjs.js"
  },
  "files": [
    "dist",
    "src",
    "!src/**/__tests__",
    "!src/**/*.{test,spec}.{js,jsx,ts,tsx}"
  ],
  "scripts": {
    "lint": "eslint src/ --fix; cspell \"**\" -c ../../cspell.config.cjs --no-progress",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@xlorem/common": "file:../common"
  }
}
