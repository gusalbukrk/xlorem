{
  "name": "@xlorem/input-validator",
  "author": "gusalbukrk",
  "version": "0.0.0",
  "description": "Validates input passed to xlorem.",
  "license": "MIT",
  "keywords": [
    "xlorem"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/bundle.cjs.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": "./dist/bundle.esm.js",
    "require": "./dist/bundle.cjs.js"
  },
  "files": [
    "dist",
    "src",
    "!src/**/__tests__",
    "!src/**/*.{test,spec}.{js,jsx,ts,tsx}"
  ],
  "scripts": {
    "build": "rollup -c ../../rollup.config.js",
    "lint": "eslint src/ --fix",
    "postbuild": "eslint dist/index.d.ts --fix --no-ignore && replace-in-file --configFile=../../.replace-in-file.cjs",
    "postpublish": "../../node_modules/rimraf/bin.js dist/",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@xlorem/common": "file:../common"
  }
}
