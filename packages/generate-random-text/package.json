{
  "name": "generate-random-text",
  "author": "gusalbukrk",
  "version": "0.0.1-alpha.0",
  "description": "Randomly generate text using given frequency map.",
  "license": "MIT",
  "keywords": [
    "random"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/bundle.cjs.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": "./dist/bundle.esm.js",
    "require": "./dist/bundle.cjs.js"
  },
  "files": [
    "dist",
    "src",
    "!src/**/__tests__",
    "!src/**/*.{test,spec}.{js,jsx,ts,tsx}"
  ],
  "scripts": {
    "build": "rollup -c ../../rollup.config.js",
    "lint": "eslint src/ --fix; cspell \"**\" -c ../../cspell.config.cjs --no-progress",
    "postbuild": "eslint dist/index.d.ts --fix --no-ignore && replace-in-file --configFile=../../.replace-in-file.cjs",
    "postpublish": "../../node_modules/rimraf/bin.js dist/",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "xlorem-common": "file:../common",
    "stopwords-utils": "file:../stopwords-utils",
    "weighted-randomness": "file:../weighted-randomness"
  }
}
